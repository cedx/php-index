--class-path src
--dce full
--define analyzer-optimize
--define js-es=6
--define no-deprecation-warnings
--define watch.excludeRoot
--library coconut.ui
--library coconut.vdom
--library tink_web
--library turnwing

--each
--define js-global=self
--js www/worker.js
--main php_index.client.ServiceWorker

--next
--define js-global=window
--js www/js/main.js
--main php_index.client.Application
--resource res/locale/en.json@locale.en.json
--resource res/locale/fr.json@locale.fr.json
